<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_import_wizard_form" model="ir.ui.view">
        <field name="name">import.wizard.form</field>
        <field name="model">import.summary.wizard</field> 
        <field name="arch" type="xml">
            <form string="Import Summaries" readonly="1">
                <group>
                    <field name="token"/> 
                </group>
                <footer>
                    <button name="action_import" type="object" string="Import" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
